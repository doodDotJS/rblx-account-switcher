{
  "name": "RBLX Account Switcher",
  "description": "A chrome extension which allows you to switch between different roblox accounts.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting", "cookies"],
  "host_permissions": ["https://www.roblox.com/*"],
  "action": {
    "default_popup": "index.html"
  }
}
